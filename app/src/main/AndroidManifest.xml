<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sh1r0.caffe_android_demo" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- CAMERA -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sh1r0.caffe_android_demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sh1r0.caffe_android_demo.ResultActivity"
            android:label="Snack Details" >
        </activity>

        <activity android:name="com.sh1r0.caffe_android_demo.CorrectActivity"
            android:label="Correct Result">
        </activity>

        <activity
            android:name="com.sh1r0.caffe_android_demo.SingleImageResult"
            android:label="Snack Details" >>
        </activity>

        <activity android:name="com.sh1r0.caffe_android_demo.AddNewActivity"
            android:label="Add New Snack">
        </activity>

    </application>

</manifest>
